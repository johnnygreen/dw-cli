{
  "name": "dw-cli",
  "version": "2.1.2",
  "description": "",
  "main": "./lib/cli.js",
  "bin": {
    "dw": "./lib/cli.js"
  },
  "engines": {
    "node": ">=11.0.0"
  },
  "scripts": {
    "test": "eslint ."
  },
  "husky": {
    "hooks": {
      "post-merge": "bash bin/post-merge",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzwallace/dw-cli.git"
  },
  "keywords": [
    "demandware",
    "dw-cli",
    "demandware cli",
    "demandware command-line interface",
    "salesforce commerce cloud"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mzwallace/dw-cli/issues"
  },
  "homepage": "https://github.com/mzwallace/dw-cli#readme",
  "dependencies": {
    "archiver": "4.0.1",
    "axios": "0.19.2",
    "bluebird": "3.7.2",
    "chalk": "4.0.0",
    "chokidar": "3.4.0",
    "cwait": "1.1.2",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "follow-redirects": "1.11.0",
    "fs-extra": "9.0.0",
    "globby": "11.0.0",
    "got": "11.1.4",
    "lodash": "4.17.15",
    "node-notifier": "7.0.0",
    "ora": "4.0.4",
    "p-retry": "4.2.0",
    "xml2js": "0.4.23",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-unicorn": "20.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": false
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": false
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "no-console": 0,
      "unicorn/no-process-exit": 0
    }
  }
}
