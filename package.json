{
  "name": "dw-cli",
  "version": "2.0.0",
  "description": "",
  "main": "./lib/cli.js",
  "bin": {
    "dw": "./lib/cli.js"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "scripts": {
    "test": "eslint .",
    "postmerge": "bash bin/post-merge",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzwallace/dw-cli.git"
  },
  "keywords": [
    "demandware",
    "dw-cli",
    "demandware cli",
    "demandware command-line interface",
    "salesforce commerce cloud"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mzwallace/dw-cli/issues"
  },
  "homepage": "https://github.com/mzwallace/dw-cli#readme",
  "dependencies": {
    "archiver": "2.1.1",
    "axios": "0.18.0",
    "bluebird": "^3.5.1",
    "chalk": "2.4.1",
    "chokidar": "^2.0.4",
    "cwait": "1.1.1",
    "debug": "3.1.0",
    "follow-redirects": "1.5.1",
    "fs-extra": "7.0.0",
    "globby": "8.0.1",
    "lodash": "^4.17.10",
    "node-notifier": "^5.2.1",
    "ora": "3.0.0",
    "p-retry": "2.0.0",
    "shelljs": "0.8.2",
    "xml2js": "0.4.19",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "eslint": "5.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7"
  },
  "ava": {
    "files": [
      "test/specs/*.js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": false
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": false
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "no-console": 0
    }
  }
}
