{
  "name": "dw-cli",
  "version": "1.0.0-rc.1",
  "description": "",
  "main": "./lib/cli.js",
  "bin": {
    "dw": "./lib/cli.js"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "test": "xo && ava",
    "postmerge": "bash bin/post-update-install",
    "precommit": "xo",
    "prepush": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzwallace/dw-cli.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mzwallace/dw-cli/issues"
  },
  "homepage": "https://github.com/mzwallace/dw-cli#readme",
  "dependencies": {
    "archiver": "^1.3.0",
    "bluebird": "^3.4.7",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "debug": "^2.6.0",
    "lodash": "^4.17.4",
    "node-notifier": "^4.6.1",
    "ora": "^1.1.0",
    "request": "^2.79.0",
    "xml2js": "0.4.17",
    "yargs": "^6.5.0"
  },
  "devDependencies": {
    "@types/archiver": "^0.15.36",
    "@types/bluebird": "^3.0.37",
    "@types/chalk": "^0.4.31",
    "@types/chokidar": "^1.4.31",
    "@types/debug": "^0.0.29",
    "@types/lodash": "^4.14.50",
    "@types/node-notifier": "^0.0.28",
    "@types/ora": "^0.3.31",
    "@types/request": "^0.0.39",
    "@types/xml2js": "^0.0.32",
    "@types/yargs": "^6.5.0",
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-xo": "0.17.0",
    "eslint-plugin-babel": "^4.0.1",
    "husky": "^0.12.0",
    "xo": "0.17.1"
  },
  "ava": {
    "files": [
      "test/specs/*.js"
    ]
  },
  "xo": {
    "space": true,
    "esnext": true,
    "ignores": [
      "test/**"
    ],
    "rules": {
      "no-use-extend-native/no-use-extend-native": 0,
      "unicorn/no-process-exit": 0,
      "import/no-dynamic-require": 0
    }
  }
